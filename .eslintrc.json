{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "rules": {
        "comma-dangle": 1,
        "no-undef": 1,
        "no-unused-vars": 1
    }
}